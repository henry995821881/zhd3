var TableDetail = function(table_details) {
	$('#Department').val(table_details.eq(0).text());
	$('#Principal').val(table_details.eq(1).text());
	$('#Organization').val(table_details.eq(2).text());
	$('#Remark').val(table_details.eq(3).text());
}
